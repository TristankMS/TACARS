    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "{SelectedTable}\r\n| where RequestSubmitted_t {TimeRange} \r\n| summarize Count=dcount(RequestID_d) by Host_s,  Requester_s, Template_s\r\n| order by Count desc",
        "size": 0,
        "showAnalytics": true,
        "title": "Unique Cert Requests by template",
        "timeContext": {
          "durationMs": 0
