      "content": {
        "version": "KqlItem/1.0",
        "query": "union withsource=[\"$TableName\"] *\r\n| where RequestSubmitted_t {TimeRange}\r\n| summarize Count=dcount(RequestID_d) by TableName=[\"$TableName\"]",
