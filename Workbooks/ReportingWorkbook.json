    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "union withsource=[\"$TableName\"] *\r\n| summarize Count=count() by TableName=[\"$TableName\"]\r\n| render barchart",
        "size": 0,
        "title": "Raw cert items uploaded per Table during timeframe",
        "timeContextFromParameter": "TimeRange",
        "showExportToExcel": true,
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces"
      },
      "customWidth": "50",
      "name": "query - 5",
      "styleSettings": {
        "padding": "4px"
      }
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "{SelectedTable}\r\n| where RequestSubmitted_t {TimeRange} \r\n| summarize Count=dcount(RequestID_d) by Host_s,  Requester_s, Template_s\r\n| order by Count desc",
        "size": 0,
        "showAnalytics": true,
        "title": "Unique Cert Requests by template",
        "showExportToExcel": true,
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces"
      },
      "customWidth": "48",
      "name": "query - 8"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "{SelectedTable}\n| summarize Count=dcount(RequestID_d) by Host_s,  Requester_s, Template_s\n| order by Count desc",
        "size": 0,
        "showAnalytics": true,
        "title": "Total unique Cert Template requests by requester",
        "showExportToExcel": true,
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces"
      },
      "customWidth": "48",
      "name": "query - 4"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "{SelectedTable}\r\n| where isempty(EKU_s) or EKU_s contains(\"Client\")\r\n| where ValidTo_t > todatetime(now())\r\n| summarize Count=count() by Type, EKU_s, Requester_s, Template_s, Subject_CN_s, SAN_s\r\n| order by Count desc",
        "size": 0,
        "showAnalytics": true,
        "title": "Certs which are still time-valid",
        "timeContextFromParameter": "TimeRange",
        "showExportToExcel": true,
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces"
      },
      "name": "query - 4"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "{SelectedTable}\r\n| where Type <> \"Usage\"\r\n| summarize Count=count() by DispositionMessage_s, Disposition_s\r\n| order by Count desc",
        "size": 1,
        "showAnalytics": true,
        "title": "Summary of Disposition Message outcomes",
        "showExportToExcel": true,
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces"
      },
      "customWidth": "48",
      "name": "query - 4"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "{SelectedTable}\r\n| where ValidTo_t > todatetime(now())\r\n| summarize by Type, ValidTo_t, Subject_CN_s, SAN_s, EKU_s, Requester_s, Template_s\r\n| order by ValidTo_t desc\r\n| take 100",
        "size": 0,
        "title": "Longest-lived certificates",
        "timeContextFromParameter": "TimeRange",
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces"
      },
      "name": "queryLongestLived"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "{SelectedTable}\r\n| where SAN_s notcontains \"DS Object GUID=\"\r\n| where ValidTo_t > todatetime(now())\r\n| summarize count() by Template_s\r\n| order by count_ desc",
        "size": 0,
        "title": "Issued certs without a DSObjectGUID",
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces"
      },
      "customWidth": "48",
      "name": "SANBreakdown"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "{SelectedTable}\r\n| where SAN_s contains \"DS Object GUID=\"\r\n| where ValidTo_t > todatetime(now())\r\n| summarize count() by Template_s\r\n| order by count_ desc",
        "size": 0,
        "title": "Issued certs with a DSObjectGUID",
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces"
      },
      "customWidth": "48",
      "name": "SANBreakdown - Copy"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "let adminUsers = datatable (name: string, tier: int)\r\n[\r\n    \"administrator\", 0,\r\n    \"ctadmin\", 0,\r\n    \"serveradmin\", 1,\r\n    \"helpdeskadmin\", 2,\r\n    \"jeff_notda\",3\r\n];\r\n{SelectedTable}\r\n| extend EKU = split(EKU_s,\"|\")\r\n| where EKU has_any (\"Smart Card Logon (1.3.6.1.4.1.311.20.2.2)\", \"Client Authentication (1.3.6.1.5.5.7.3.2)\")\r\n| extend SAN = tostring(split(SAN_s,\"|\"))\r\n| extend PrincipalName = extract(\"Principal Name=([\\\\w@\\\\._\\\\+]*)\",1,SAN)\r\n| extend RFC822Name=tostring(split(SAN_s,\"RFC822 Name=\")[1])\r\n| where SAN has_any (adminUsers)\r\n| summarize by Type, TimeGenerated,Host_s,RequestID_d,Template_s,RequestSubmitted_t,SAN_s,EKU_s,Requester_s,Disposition_s,Serial_s,Subject_CN_s,ValidFrom_t,ValidTo_t",
        "size": 0,
        "title": "Client Auth certificates for sensitive users",
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces",
        "gridSettings": {
          "sortBy": [
            {
              "itemKey": "TimeGenerated",
              "sortOrder": 2
            }
          ]
        },
        "sortBy": [
          {
            "itemKey": "TimeGenerated",
            "sortOrder": 2
          }
        ]
      },
      "name": "SensitiveClientAuth"
    }
