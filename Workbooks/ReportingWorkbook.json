    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "union withsource=[\"$TableName\"] *\r\n| where RequestSubmitted_t {TimeRange}\r\n| summarize Count=dcount(RequestID_d) by TableName=[\"$TableName\"]",
        "size": 0,
        "title": "Cert items with RequestSubmitted since time range",
        "showRefreshButton": true,
        "exportFieldName": "series",
        "exportParameterName": "SelectedTable",
        "exportDefaultValue": "union withsource=[\"$TableName\"] *",
        "showExportToExcel": true,
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces",
        "visualization": "barchart",
        "gridSettings": {
          "formatters": [
            {
              "columnMatch": "TableName",
              "formatter": 7,
              "formatOptions": {
                "linkTarget": "CellDetails",
                "linkIsContextBlade": true
